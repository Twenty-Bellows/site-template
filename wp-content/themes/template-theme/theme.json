{
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"layout": {
			"contentSize": "42rem",
			"wideSize": "64rem"
		},
		"color": {
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"slug": "base",
					"color": "#ffffff",
					"name": "Base"
				},
				{
					"slug": "surface",
					"color": "#f5f5f5",
					"name": "Surface"
				},
				{
					"slug": "surface-variant",
					"color": "#e8e8e8",
					"name": "Surface Variant"
				},
				{
					"slug": "text",
					"color": "#1a1a1a",
					"name": "Text"
				},
				{
					"slug": "text-muted",
					"color": "#666666",
					"name": "Text Muted"
				},
				{
					"slug": "primary",
					"color": "#333333",
					"name": "Primary"
				},
				{
					"slug": "primary-hover",
					"color": "#000000",
					"name": "Primary Hover"
				},
				{
					"slug": "accent",
					"color": "#4a4a4a",
					"name": "Accent"
				},
				{
					"slug": "border",
					"color": "#d4d4d4",
					"name": "Border"
				}
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"slug": "system",
					"name": "System"
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": "0.875rem",
					"name": "Small",
					"fluid": {
						"min": "0.875rem",
						"max": "0.875rem"
					}
				},
				{
					"slug": "base",
					"size": "1rem",
					"name": "Base",
					"fluid": {
						"min": "1rem",
						"max": "1rem"
					}
				},
				{
					"slug": "medium",
					"size": "clamp(1.125rem, 1rem + 0.625vw, 1.25rem)",
					"name": "Medium",
					"fluid": {
						"min": "1.125rem",
						"max": "1.25rem"
					}
				},
				{
					"slug": "large",
					"size": "clamp(1.5rem, 1.25rem + 1.25vw, 2rem)",
					"name": "Large",
					"fluid": {
						"min": "1.5rem",
						"max": "2rem"
					}
				},
				{
					"slug": "x-large",
					"size": "clamp(2rem, 1.5rem + 2.5vw, 3rem)",
					"name": "Extra Large",
					"fluid": {
						"min": "2rem",
						"max": "3rem"
					}
				}
			]
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"slug": "xs",
					"size": "0.5rem",
					"name": "Extra Small"
				},
				{
					"slug": "sm",
					"size": "1rem",
					"name": "Small"
				},
				{
					"slug": "base",
					"size": "1.5rem",
					"name": "Base"
				},
				{
					"slug": "md",
					"size": "2rem",
					"name": "Medium"
				},
				{
					"slug": "lg",
					"size": "3rem",
					"name": "Large"
				},
				{
					"slug": "xl",
					"size": "4rem",
					"name": "Extra Large"
				},
				{
					"slug": "2-xl",
					"size": "6rem",
					"name": "2X Large"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--text)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system)",
			"fontSize": "var(--wp--preset--font-size--base)",
			"lineHeight": "1.6"
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--base)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--base)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)"
				},
				"spacing": {
					"padding": {
						"top": "0.75rem",
						"right": "1.5rem",
						"bottom": "0.75rem",
						"left": "1.5rem"
					}
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--primary-hover)"
					}
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary-hover)"
					}
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "600",
					"lineHeight": "1.2"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			}
		},
		"blocks": {
			"core/separator": {
				"color": {
					"background": "var(--wp--preset--color--border)"
				}
			}
		}
	},
	"templateParts": [
		{
			"name": "header",
			"area": "header"
		}
	]
}
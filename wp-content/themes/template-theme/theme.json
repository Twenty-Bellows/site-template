{
	"$schema": "https://schemas.wp.org/wp/6.9/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#f5f5f5",
					"name": "Surface",
					"slug": "surface"
				},
				{
					"color": "#e8e8e8",
					"name": "Surface Variant",
					"slug": "surface-variant"
				},
				{
					"color": "#1a1a1a",
					"name": "Text Default",
					"slug": "text-default"
				},
				{
					"color": "#666666",
					"name": "Text Muted",
					"slug": "text-muted"
				},
				{
					"color": "#333333",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#000000",
					"name": "Primary Hover",
					"slug": "primary-hover"
				},
				{
					"color": "#4a4a4a",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#d4d4d4",
					"name": "Border",
					"slug": "border"
				}
			]
		},
		"layout": {
			"contentSize": "42rem",
			"wideSize": "64rem"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Extra Small",
					"size": "0.5rem",
					"slug": "xs"
				},
				{
					"name": "Small",
					"size": "1rem",
					"slug": "sm"
				},
				{
					"name": "Base",
					"size": "1.5rem",
					"slug": "base"
				},
				{
					"name": "Medium",
					"size": "2rem",
					"slug": "md"
				},
				{
					"name": "Large",
					"size": "3rem",
					"slug": "lg"
				},
				{
					"name": "Extra Large",
					"size": "4rem",
					"slug": "xl"
				},
				{
					"name": "2X Large",
					"size": "6rem",
					"slug": "2-xl"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System",
					"slug": "system"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"max": "0.875rem",
						"min": "0.875rem"
					},
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1rem",
						"min": "1rem"
					},
					"name": "Base",
					"size": "1rem",
					"slug": "base"
				},
				{
					"fluid": {
						"max": "1.25rem",
						"min": "1.125rem"
					},
					"name": "Medium",
					"size": "clamp(1.125rem, 1rem + 0.625vw, 1.25rem)",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "2rem",
						"min": "1.5rem"
					},
					"name": "Large",
					"size": "clamp(1.5rem, 1.25rem + 1.25vw, 2rem)",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "3rem",
						"min": "2rem"
					},
					"name": "Extra Large",
					"size": "clamp(2rem, 1.5rem + 2.5vw, 3rem)",
					"slug": "x-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/separator": {
				"color": {
					"background": "var(--wp--preset--color--border)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--text-default)"
		},
		"elements": {
			"button": {
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--primary-hover)"
					}
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--base)"
				},
				"spacing": {
					"padding": {
						"bottom": "0.75rem",
						"left": "1.5rem",
						"right": "1.5rem",
						"top": "0.75rem"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "600",
					"lineHeight": "1.2"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary-hover)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--base)",
			"padding": {
				"bottom": "0px",
				"left": "1rem",
				"right": "1rem",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system)",
			"fontSize": "var(--wp--preset--font-size--base)",
			"lineHeight": "1.6"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	]
}